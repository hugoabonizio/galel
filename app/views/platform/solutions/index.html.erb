<h1>Listing platform_solutions</h1>

<table>
  <tr>
    <th>Version</th>
    <th>Status</th>
    <th>Solution</th>
    <th>Attach</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @platform_solutions.each do |platform_solution| %>
  <tr>
    <td><%= platform_solution.version %></td>
    <td><%= platform_solution.status %></td>
    <td><%= platform_solution.solution %></td>
    <td><%= platform_solution.attach %></td>
    <td><%= link_to 'Show', platform_solution %></td>
    <td><%= link_to 'Edit', edit_platform_solution_path(platform_solution) %></td>
    <td><%= link_to 'Destroy', platform_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Solution', new_platform_solution_path %>
